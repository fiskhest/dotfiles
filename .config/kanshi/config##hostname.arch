profile {
        output eDP-1 mode 1920x1200@60Hz enable
        exec hyprctl keyword general:gaps_in  3
        exec hyprctl keyword general:gaps_out 4
        exec launch-waybar
        exec ln -sf ~/.config/swaylock/config.laptop ~/.config/swaylock/config
}
